<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:w="urn:import:com.sonnyproject.client.widget">

	<g:HTMLPanel ui:field="scrollPanel">
		<g:HTMLPanel>
			<h1 style="font-face:Arial;">CONTACT</h1>
		</g:HTMLPanel>
		<w:DataField ui:field="name"></w:DataField>
		<w:DataField ui:field="email"></w:DataField>
		<w:CommentField ui:field="comment"></w:CommentField>
	</g:HTMLPanel>
</ui:UiBinder> 